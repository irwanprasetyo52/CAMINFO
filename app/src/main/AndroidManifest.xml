<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.com.caminfo">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity
                android:name=".SplashScreen"
                android:label="@string/app_name"
                android:noHistory="true"
                android:screenOrientation="portrait"
                android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity"/>
        <activity
                android:name=".Beranda"
                android:label="@string/title_activity_beranda"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
                android:name=".ProfileP"
                android:label="@string/profile_mahasiswa"
                android:parentActivityName=".Beranda">
            <meta-data
                    android:name="android.support.PARENT_ACTIVITY"
                    android:value=".Beranda"/>
        </activity>
        <activity
                android:name=".Profile"
                android:label="@string/title_activity_profile"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
                android:name=".KHS"
                android:label="@string/title_activity_khs"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
                android:name=".Jadwal"
                android:label="@string/title_activity_jadwal"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
                android:name=".Tugas"
                android:label="@string/title_activity_tugas"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
                android:name=".Presensi"
                android:label="@string/title_activity_presensi"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
                android:name=".Beasiswa"
                android:label="@string/title_activity_beasiswa"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".RincianBidikmisi">
        </activity>
    </application>

</manifest>